{
  "title": "Sessions & Adaptation Overview",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "timezone": "browser",
  "panels": [
    {
      "type": "row",
      "title": "Throughput",
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Session Events / Started",
      "targets": [
        { "expr": "rate(session_events_total[5m])", "legendFormat": "events" },
        { "expr": "rate(sessions_started_total[5m])", "legendFormat": "started" }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 }
    },
    {
      "type": "timeseries",
      "title": "Adaptation Call Rate by Status",
      "targets": [
        { "expr": "sum by (status) (rate(adaptation_call_latency_seconds_count[5m]))", "legendFormat": "{{status}}" }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 }
    },
    {
      "type": "row",
      "title": "Latency",
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Session Event Ingest p95",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(session_event_ingest_latency_seconds_bucket[5m])) by (le))", "legendFormat": "p95" }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 }
    },
    {
      "type": "timeseries",
      "title": "Adaptation Call p95 (ok)",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(adaptation_call_latency_seconds_bucket{status=\"ok\"}[5m])) by (le))", "legendFormat": "p95" }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 }
    },
    {
      "type": "row",
      "title": "Reliability",
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Circuit State",
      "targets": [ { "expr": "sessions_adaptation_circuit_state", "legendFormat": "state" } ],
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 17 }
    },
    {
      "type": "timeseries",
      "title": "Rate Limited Requests",
      "targets": [ { "expr": "increase(sessions_rate_limited_total[5m])", "legendFormat": "rate_limited" } ],
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 17 }
    },
    {
      "type": "timeseries",
      "title": "SSE Heartbeats & Disconnects",
      "targets": [
        { "expr": "rate(sessions_sse_heartbeats_total[5m])", "legendFormat": "heartbeats" },
        { "expr": "rate(sessions_sse_disconnects_total[5m])", "legendFormat": "disconnects" }
      ],
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 17 }
    }
  ],
  "templating": { "list": [] },
  "annotations": { "list": [] }
}
